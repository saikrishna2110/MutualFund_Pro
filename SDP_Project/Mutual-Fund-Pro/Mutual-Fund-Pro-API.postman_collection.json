            "body": {
              "mode": "raw",
              "raw": "{\n    \"username\": \"testuser_{{timestamp}}\",\n    \"email\": \"testuser_{{timestamp}}@example.com\",\n    \"password\": \"password123\",\n    \"firstName\": \"Test\",\n    \"lastName\": \"User\",\n    \"role\": \"Investor\",\n    \"riskProfile\": \"Moderate\"\n}"
